extends ../../core/pug/_layout


block content

	//=============== block-0 ===============
	.block-0

		header.main-header
			.container-fluid
				.main-header__inner

					.main-header__left

						.logo
							a(href="#" target="_blank" rel="noopener noreferrer")
								img.logo__img(src="img/logo.png", alt="logo")
						
						.main-header__menu
							nav
								button.main-header__menu-item(disabled="disabled") Олимпиады
								button.main-header__menu-item Разработки

					.main-header__right
						.main-header__right-login
							a.butn.butn--login.js_open_modal(href="#js_login" target="_blank" rel="noopener noreferrer") Войти
						//- .main-header__right-user
							.user
								.user__name
									p.user__name-text Анастасия
								.user__ava
									img.user__ava-img(src="img/ava-1.jpg", alt="avatar")

								.user__menu
									nav.user__menu-list
										a.user__menu-item(href="#" target="_blank" rel="noopener noreferrer") Мои заявки
											+svg("icon-menu-request", "menu-icon")
										a.user__menu-item.is-active(href="#" target="_blank" rel="noopener noreferrer") Мой профиль
											+svg("icon-menu-user", "menu-icon")
										a.user__menu-item(href="#" target="_blank" rel="noopener noreferrer") Арихив
											+svg("icon-menu-archive", "menu-icon")
										a.user__menu-item(href="#" target="_blank" rel="noopener noreferrer") Выйти
											+svg("icon-menu-exit", "menu-icon")



		.title-field
			.container-fluid
				.title-field__inner.bg-items-place
					.row
						.col-lg

							.title-field__wrap

								.main-title
									.main-title__inner
										h2.main-title__text-2 Международные #[b онлайн олимпиады] по математике
										h1.main-title__text «Мир математики»
										h2.main-title__text-3 с 1 по 11 классы
										p.main-title__text-4.fz-medium Организатор ООО «КОМПЭДУ», УНН  790867878

								.info-title
									.info-title__item.info-title__item-date
										.info-title__item-icon
											+svg("icon-header-calendar", "info-icon")
										.info-title__text
											p.text-1 Проведение олимпиад
											p.text-2 с 11 декабря по 28 февраля

									.info-title__item.info-title__item-price
										.info-title__item-icon
											+svg("icon-header-wallet", "info-icon")
										.info-title__text
											p.text-1 Оргвзнос
											p.text-2 59 рублей

								.butn-wrap
									button.butn.butn--xl.butn--green.js_open_modal(href="#js_popup-request--1") Принять участие

						.col-lg.hidden-md-down

							.advantages
								ul.advantages__list
									li.advantages__item.advantages__item--1 Простое и удобное участие
									li.advantages__item.advantages__item--2 Доступная стоимость
									li.advantages__item.advantages__item--3 Полезные подарки


							.superhero-1
								.superhero-1__wrap
									img.superhero-1__img(src="img/mathman1.png", alt="mathman1")
					.planets
						.planet.planet--1
						.planet.planet--2
						.planet.planet--3
					
					.meteors
						.meteor.meteor--1
						.meteor.meteor--2
						.meteor.meteor--3
						.meteor.meteor--4

					.nums
						.num.num--6
						.num.num--4
						.num.num--0
						.num.num--7
						.num.num--8
						.num.num--9
						.num.num--5

	//=============== /block-0 ===============



	//=============== /block-1 ===============
	.block-1

		.container
			.row
				.col-lg
					.block-1__inner.bg-items-place

						.limit

							.title
								h3.title__text Приветствуем вас на сайте онлайн олимпиад по математике
				
								p.title__descr Проведите олимпиаду вместе с нашим проектом! У вас разбегутся глаза от количества разнообразных и занимательных заданий, которые помогут ученикам сосредоточить своё внимание на учёбе и просто весело и с пользой провести время.

								p.title__descr Участвовать легко и удобно! Просто укажите количество учеников, их имена и фамилии, внесите ответы и сразу получите результаты и награды.


							.tiles
								.tiles__inner
									
									.tile.tile--1
										.tile__title
											h4.tile__title-text Простое и удобное участие
										.tile__descr
											p.tile__descr-text.fz-medium Минимальные затраты ваших сил и времени, чтобы подать заявку, ввести ответы и получить результаты.
									
									.tile.tile--2
										.tile__title
											h4.tile__title-text Доступная стоимость
										.tile__descr
											p.tile__descr-text.fz-medium Чем больше учеников вы привлекаете, тем меньше сумма оргвзноса.

									.tile.tile--3
										.tile__title
											h4.tile__title-text Занимательные задания
										.tile__descr
											p.tile__descr-text.fz-medium Интересные, красочные задания разной степени сложности подойдут всем участникам.

									.tile.tile--4
										.tile__title
											h4.tile__title-text Красочные наградные
										.tile__descr
											p.tile__descr-text.fz-medium Бесплатные свидетельства и благодарности для учителей и родителей, дипломы и сертификаты для победителей и участников.

									.tile.tile--5
										.tile__title
											h4.tile__title-text Подарок каждому участнику
										.tile__descr
											p.tile__descr-text.fz-medium Ни один участник не останется без полезного и интересного подарка.

									.tile.tile--6
										.tile__title
											h4.tile__title-text Благодарность родителям
										.tile__descr
											p.tile__descr-text.fz-medium Родители всех участников получат специальную благодарность.
							.nums
								.num-big.num-big--4
								.num-big.num-big--eq
								.num-big.num-big--5
								.num-big.num-big--7

	//=============== /block-1 ===============



	//=============== block-2 ===============
	.block-2
		.container
			.row
				.col-lg

					.reward
						.reward__inner

							.reward__title
								h2.reward__title-text Награды учителям и координаторам

							.reward__descr
								p.reward__descr-text.fz-medium Каждый учитель или координатор делает очень полезную работу, привлекая учеников к участию в олимпиадах. Именно поэтому мы подготовили для вас эти подарки и поощрения.

							.reward__list
								ul
									li.reward__list-item
										h4.reward__list-title Бесплатные благодарственные письма
										p.reward__list-descr.fz-medium Всем учителям и координаторам.

									li.reward__list-item
										h4.reward__list-title Бесплатные свидетельства о подготовке победителя или призёра.
										p.reward__list-descr.fz-medium Если у вас более 10 участников.

									li.reward__list-item
										h4.reward__list-title Благодарность учреждению образования.
										p.reward__list-descr.fz-medium Если у вас более 5 участников.
				
				.col-lg
					.reward-gallery
						.reward-gallery__item.reward-gallery__item--1
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/f2f2f2?text=PICTURE-1", alt="picture")
						.reward-gallery__item.reward-gallery__item--2
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/ededed?text=PICTURE-2", alt="picture")
						.reward-gallery__item.reward-gallery__item--3
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/e6e6e6?text=PICTURE-3", alt="picture")

	//=============== /block-2 ===============



	//=============== block-3 ===============
	.block-3
		.container
			.row
				.col-lg.order-top
					.gift-gallery
						.gift-gallery__item.gift-gallery__item--1
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/e6e6e6?text=PICTURE-1", alt="picture")
						.gift-gallery__item.gift-gallery__item--2
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/ededed?text=PICTURE-2", alt="picture")
						.gift-gallery__item.gift-gallery__item--3
							img.reward-gallery__img(src="http://via.placeholder.com/477x336/f2f2f2?text=PICTURE-3", alt="picture")

				.col-lg

					.gift
						.gift__inner

							.gift__title
								h2.gift__title-text Подарки победителям и участникам

							.gift__descr
								p.gift__descr-text.fz-medium Ученики не получают зарплату, но мы прекрасно понимаем, что они стараются и должны быть вознаграждены. Для этого мы и подготовили эти интересные и полезные подарки.

							.gift__list
								ul
									li.gift__list-item
										h4.gift__list-title Бесплатные дипломы и сертификаты всем участникам.
									li.gift__list-item
										h4.gift__list-title Благодарность родителям.
									li.gift__list-item
										h4.gift__list-title Тематическая настольная игра «Школьный марафон».
									li.gift__list-item
										h4.gift__list-title Занимательные и доступные задания.
									li.gift__list-item
										h4.gift__list-title Возможность узнать свои ошибки и посмотреть решения.

