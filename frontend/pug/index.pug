	//=============== block-4 ===============
	.block-4
		.container
			.row
				.col-lg
					.block-4__inner.bg-items-place

						.butn-wrap
							button.butn.butn--xl.butn--green.js_open_modal(href="#js_popup-request--1") Принять участие

						

						.planets
							.planet.planet--4
							.planet.planet--5
						.meteors
							.meteor.meteor--5
							.meteor.meteor--6

