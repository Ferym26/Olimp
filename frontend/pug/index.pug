	//=============== POP-UPs ===============

	.overlay#js_overlay

	//LOGIN
	.popup.popup--login.js_popup#js_login
		.popup__inner
			.popup__head
				h4.popup__head-text Войти в мой профиль
				.popup__close.js_popup_close

			.popup__body
				form.popup__form.form(action="" method="")
					.popup__form-item
						label.label(for="email") Ваш e-mail
						input.input.input--lg(type="text", id="email", placeholder="mymail@mail.com")
					.popup__form-item
						label.label(for="password") Пароль
						.input-wrap
							input.input.input--lg.input--is-error(type="password", id="password", placeholder="• • • • • • • • • • • •")
							+svg("icon-pass", "eye-icon")
							span.validation-error Пользователь не найден

					.popup__form-item
						a.link-blue.fz-spec(href="") Забыли пароль?

					.butn-wrap
						button.butn.butn--md.butn--turq-green Войти
			
			.popup__footer
				.popup__footer-reg
					p.popup__footer-reg-title Впервые у нас?
					a.link-green.js_open_modal(href="#js_register") Зарегистрируйтесь!


	//REGISTR
	.popup.popup--registr.js_popup#js_register
		.popup__inner
			.popup__head
				h4.popup__head-text Регистрация
				.popup__close.js_popup_close

			.popup__body
				form.popup__form.form(action="" method="")
